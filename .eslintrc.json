{
  "plugins": ["github"],
  "extends": [
    "plugin:github/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "globals": {
    "RemoteInputElement": "readonly",
    "RequestInit": true
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
